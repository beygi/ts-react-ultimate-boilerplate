{
  "name": "ts-react-ultimate-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "sideEffects": true,
  "license": "MIT",
  "scripts": {
    "build": "yarn run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rm -f -r -d dist",
    "lint": "yarn run lint:ts && yarn run lint:sass",
    "lint:ts": "tslint './src/**/*.ts*' --format stylish --force",
    "lint:sass": "stylelint ./src/**/*.scss",
    "start": "yarn run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev.js",
    "start-prod": "yarn run build && http-server dist",
    "test": "jest --watch --coverage --config=configs/jest.json",
    "profile": "webpack -p --config=configs/webpack/prod.js --profile --json > webpack-stats.json",
    "extract": "nodejs bin/extract.js && ls -1 src/services/trans/*.po | xargs -i msgmerge -U --backup=off --no-wrap {} src/services/trans/translation.pot",
    "apiGen": "ls swager-specs -1 | grep json | xargs -i node ./src/lib/api/generate.js {} && tslint --fix --force src/lib/api/*.ts && tslint --fix --force src/lib/api/*.ts",
    "doc": "typedoc --tsconfig ./tsconfig.json --readme ./README.md --json docs/docs.json --out docs --mode modules ./src"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/free-regular-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@types/i18next": "^12.1.0",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.134",
    "@types/node": "^12.0.7",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.9",
    "@types/react-router": "^5.0.1",
    "@types/react-router-dom": "^4.3.3",
    "@types/webpack-env": "^1.13.9",
    "antd": "^3.19.2",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "gettext-extractor": "^3.4.3",
    "html-webpack-plugin": "^3.2.0",
    "i18next": "^15.1.3",
    "i18next-po-loader": "^1.0.1",
    "image-webpack-loader": "^4.6.0",
    "install": "^0.12.2",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "npm": "^6.14.6",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.9.0",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "speed-measure-webpack-plugin": "^1.3.1",
    "style-loader": "^0.23.1",
    "tinycolor2": "^1.4.1",
    "tslint": "^5.18.0",
    "tslint-loader": "^3.6.0",
    "typedoc": "^0.14.1",
    "typedoc-plugin-external-module-name": "^2.1.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.33.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "antd-jalali-moment": "^3.3.2",
    "connected-react-router": "^6.4.0",
    "git-revision-webpack-plugin": "^3.0.3",
    "http-server": "^0.11.1",
    "strftime": "^0.10.0",
    "swagger-typescript-codegen": "^1.10.0"
  }
}
